<?xml version="1.0" encoding="utf-8" ?>
<!--
***********************************************************************************************
MSbuild target that automatically signs the referenced DryIoc packages using a powershell script. 
***********************************************************************************************
-->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
   <Target Name="SignDryioc" BeforeTargets="Build">
      <Exec Command='powershell  -ExecutionPolicy unrestricted -File "$(MSBuildThisFileDirectory)..\tools\SignDryIoc.ps1" "$(MSBuildProjectFullPath)" "$(MSBuildProjectDirectory)"' />
   </Target>
</Project>
